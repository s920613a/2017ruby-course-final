<%= simple_form_for bike do |f| %>
  <%= f.input :bikeType, label:'車種型號' %><br />
  <%= f.input :quantity, label:'數量' %><br />
  <%= f.association :supplier, label_method: :supName, value_method: :id, label:'供應商編號' %><br />
  <div style="text-align:center;">
    <%= f.button:submit, "新增車種", class:"btn btn-success btn-lg"%>
    <%= link_to "返回", bikes_path, class: "btn btn-info btn-lg" %>
  </div>
<% end %>
