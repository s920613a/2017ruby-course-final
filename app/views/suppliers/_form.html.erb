<%= simple_form_for supplier do |f| %>
  <%= f.input :supName, label: '供應商名稱' %><br />
  <%= f.input :supTel, label: '電話' %><br />
  <div style="text-align:center;">
    <%= f.button:submit, "新增供應商", class:"btn btn-success btn-lg" %>
    <%= link_to "返回", suppliers_path, class: "btn btn-info btn-lg" %>
  </div>
<% end %>
